<HomePage>  :                   # this is the class which will contain all the widgets of the app. 
    name : 'Quiz'
    canvas.before :
        Rectangle :
            size : self.size
            source : 'modern_fam.jpg'
    
    Button :
        on_release : app.root.current = 'q1'         # tells the button which page to go to upon being pressed. 
        size_hint : 0.2, 0.2                        # this determines the size of the button wrt to the size of the screen.
        pos_hint : {'x' : 0.01, 'y' : 0.4}          # this determines the position of the button on the screen, default = bottom-left
        text : "Play"


<q1>
    name : 'q1'
    canvas.before :
        Rectangle :
            size : self.size
        Color :
            rgba : 0,255,255,0.2

    GridLayout :
        cols : 2
        size : root.width, root.height


        Label :
            text : "What is your favorite color?"
            font_size : 30
            font_name : "Times.ttf"             # can change the font here
            color : 0,0,0,1

        Label :
            text : ' '

        
        Label :
            background_color : (22/255, 201/255, 250/255, 1)
            canvas.before :
                Color :
                    rgba : self.background_color
                
                Rectangle : 
                    size : self.size
                    pos : self.pos
                
            text : "Blue"
            font_size : 30
            color : (0,0,0,1)

        CheckBox:
            group : "Color"
            on_release : root.optionClicked(self, self.active, 'A')
            on_release : app.root.current = 'q2'
            color : 0,0,0,1
        
        Label :
            background_color : (40/255, 222/255, 49/255, 1)
            canvas.before :
                Color :
                    rgba : self.background_color
                
                Rectangle : 
                    size : self.size
                    pos : self.pos
            text : "Green"
            font_size : 30
            color : (0,0,0,1)

        CheckBox:
            group : "Color"
            on_release : root.optionClicked(self, self.active, 'Cam')
